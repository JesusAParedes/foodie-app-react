    {
        "version": 2,
        "builds": [
            {
                "src": "./src/index.js",
                "use": "@vercel/react"
            }
        ],
        "routes": [
            {
                "src": "/(.*)",
                "dest": "./src/index.js",
                "methods": [
                    "GET",
                    "POST",
                    "PUT",
                    "DELETE",
                    "OPTIONS"
                ],
                "headers": {
                    "Access-Control-Allow-Origin": "*"
                }
            }
        ],
        "trailingSlash": false,
        "cleanUrls": true,
        "rewrites": [
            {
                "source": "/(.*)",
                "destination": "/"
            }
        ]
    }